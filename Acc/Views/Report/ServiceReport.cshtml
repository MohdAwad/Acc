<script>
    function GetSelectedAccount(data) {
        if (data.AccountNumber != null && data.AccountNumber != '') {
            document.getElementById("AccountNumber").value = data.AccountNumber;
            document.getElementById("AccountName").value = data.AccountName;
        }
        else {
            document.getElementById("AccountNumber").value = '';
            document.getElementById("AccountName").value = '';
        }

    }
    function GetSelectedService(data) {
        if (data.ServiceID != null && data.ServiceName != '') {
            document.getElementById("ServiceNo").value = data.ServiceID;
            document.getElementById("ServiceName").value = data.ServiceName;
        }
        else {
            document.getElementById("ServiceNo").value = '';
            document.getElementById("ServiceName").value = '';
        }

    }
    var ServiceNumber = [];
    $.getJSON('@Url.Action("GetAllServices", "MultiUse")', { id: 0 }, function (Data) {
        ServiceNumber = [];
            for (var i = 0, len = Data.length; i < len; i++) {
                var r = { label: Data[i].ServiceName, value: Data[i].ServiceID };
                ServiceNumber.push(r);
            }
    });
    var AccNumber = [];
    $.getJSON('@Url.Action("GetAcountAcc", "MultiUse")', { id: 0 }, function (Data) {
        AccNumber = [];
            for (var i = 0, len = Data.length; i < len; i++) {
                var r = { label: Data[i].AccountName, value: Data[i].AccountNumber, stop: Data[i].StoppedAccount };
                AccNumber.push(r);
            }
    });
    $(function () {
        $("#ServiceNo").autocomplete({
            source: ServiceNumber,
            select: function (event, ui) {
                document.getElementById("ServiceNo").value = ui.item.value;
                document.getElementById("ServiceName").value = ui.item.label;
            }
        });
    });
    $(function () {
        $("#AccountNumber").autocomplete({
            source: AccNumber,
            select: function (event, ui) {
                document.getElementById("AccountNumber").value = ui.item.value;
                document.getElementById("AccountName").value = ui.item.label;
            }
        });
    });
</script>
