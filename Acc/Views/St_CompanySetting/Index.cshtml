@{
    ViewBag.Title = @Resources.Resource.WarehouseSettings;
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<style>
    .btn-text {
        font-family: Verdana, Geneva, Tahoma, sans-serif
    }

    .panel-title {
        font-family: Verdana, Geneva, Tahoma, sans-serif
    }
</style>
<div class="panel card-view">
    <div class="panel-heading" style="background-color: #34495E">
        <div class="pull-left">
            <h6 class="panel-title txt-light" style="font-weight:bold">@Resources.Resource.WarehouseSettings</h6>
        </div>
        <div class="clearfix"></div>
    </div>
    <div class="panel-wrapper collapse in">
        <div class="panel-body">
            <div class="row">
                <div class="col-sm-2 col-xs-12 mt-15">
                    <a href="/St_Description/Index" class="btn btn-rounded btn-block btn-anim" style="background-color:#E74C3C"><i class="fa fa-th"></i><span class="btn-text">@Resources.Resource.Descriptions</span></a>
                </div>
                <div class="col-sm-2 col-xs-12 mt-15">
                    <a href="/St_Warehouse/Index" class="btn btn-rounded btn-block btn-anim" style="background-color:#2980B9"><i class="fa fa-th"></i><span class="btn-text">@Resources.Resource.Warehouses</span></a>
                </div>
                <div class="col-sm-2 col-xs-12 mt-15">
                    <a href="/St_ItemCard/Index" class="btn btn-rounded btn-block btn-anim" style="background-color:#1ABC9C"><i class="fa fa-th"></i><span class="btn-text">@Resources.Resource.ItemCard</span></a>
                </div>
            </div>
            <div class="row">
                <div class="col-sm-2 col-xs-12 mt-15">
                    <a href="/St_DescriptionDetail/Index" class="btn btn-rounded btn-block btn-anim" style="background-color:#E74C3C"><i class="fa fa-th"></i><span class="btn-text">@Resources.Resource.DescriptionsDetails</span></a>
                </div>
                <div class="col-sm-2 col-xs-12 mt-15">
                    <a href="/St_CompanyTransactionKind/Index" class="btn btn-rounded btn-block btn-anim" style="background-color:#2980B9"><i class="fa fa-th"></i><span class="btn-text">@Resources.Resource.WarehouseCompanyTransactionKind</span></a>
                </div>
                <div class="col-sm-2 col-xs-12 mt-15">
                    <a href="/St_Warehouse/SaveSt_ItemWarehouse" class="btn btn-rounded btn-block btn-anim" style="background-color:#1ABC9C"><i class="fa fa-th"></i><span class="btn-text">@Resources.Resource.LinkingItemsToWarehouses</span></a>
                </div>
            </div>
            <div class="row">
                <div class="col-sm-2 col-xs-12 mt-15">
                    <a href="/St_CategoryPrice/Index" class="btn btn-rounded btn-block btn-anim" style="background-color:#E74C3C"><i class="fa fa-th"></i><span class="btn-text">@Resources.Resource.CategoriesPrices</span></a>
                </div>
                <div class="col-sm-2 col-xs-12 mt-15">
                    <a href="/St_WarehouseAccount/SaveSt_WarehouseAccount" class="btn btn-rounded btn-block btn-anim" style="background-color:#2980B9"><i class="fa fa-th"></i><span class="btn-text">@Resources.Resource.WarehouseAccounts</span></a>
                </div>
                <div class="col-sm-2 col-xs-12 mt-15">
                    <a href="/St_AdvertisingRepresentative/SaveSt_ItemAdvertisingRepresentative" class="btn btn-rounded btn-block btn-anim" style="background-color:#1ABC9C"><i class="fa fa-th"></i><span class="btn-text">@Resources.Resource.LinkingItemsToAdvertisingRepresentative</span></a>
                </div>
            </div>
            <div class="row">
                <div class="col-sm-2 col-xs-12 mt-15">
                    <a href="/St_MeasurementDetail/Index" class="btn btn-rounded btn-block btn-anim" style="background-color:#E74C3C"><i class="fa fa-th"></i><span class="btn-text">@Resources.Resource.MeasurementsDetails</span></a>
                </div>
                <div class="col-sm-2 col-xs-12 mt-15">
                    <a href="/St_OtherAccount/SaveSt_OtherAccount" class="btn btn-rounded btn-block btn-anim" style="background-color:#2980B9"><i class="fa fa-th"></i><span class="btn-text">@Resources.Resource.OtherAccounts</span></a>
                </div>
            </div>
            <div class="row">
                <div class="col-sm-2 col-xs-12 mt-15">
                    <a href="/St_AdvertisingRepresentative/Index" class="btn btn-rounded btn-block btn-anim" style="background-color:#E74C3C"><i class="fa fa-th"></i><span class="btn-text">@Resources.Resource.AdvertisingRepresentatives</span></a>
                </div>
                <div class="col-sm-2 col-xs-12 mt-15">
                    <a href="/St_Warehouse/DetermineWarehouseQuantities" class="btn btn-rounded btn-block btn-anim" style="background-color:#2980B9"><i class="fa fa-th"></i><span class="btn-text">@Resources.Resource.DetermineWarehouseQuantities</span></a>
                </div>
            </div>
            <div class="row">
                <div class="col-sm-2 col-xs-12 mt-15">
                    <a href="/St_CountryOfOrigin/Index" class="btn btn-rounded btn-block btn-anim" style="background-color:#E74C3C"><i class="fa fa-th"></i><span class="btn-text">@Resources.Resource.CountryOfOrigin</span></a>
                </div> 
            </div>
            <div class="row">
                <div class="col-sm-2 col-xs-12 mt-15">
                    <a href="/St_ManufacturerCompany/Index" class="btn btn-rounded btn-block btn-anim" style="background-color:#E74C3C"><i class="fa fa-th"></i><span class="btn-text">@Resources.Resource.ManufacturerCompanies</span></a>
                </div>
            </div>
            <div class="row">
                <div class="col-sm-2 col-xs-12 mt-15">
                    <a href="/St_ItemUnit/Index" class="btn btn-rounded btn-block btn-anim" style="background-color:#E74C3C"><i class="fa fa-th"></i><span class="btn-text">@Resources.Resource.ItemUnits</span></a>
                </div>
            </div>
            <div class="row">
                <div class="col-sm-2 col-xs-12 mt-15">
                    <a href="/St_Driver/Index" class="btn btn-rounded btn-block btn-anim" style="background-color:#E74C3C"><i class="fa fa-th"></i><span class="btn-text">@Resources.Resource.Drivers</span></a>
                </div>
            </div>
            <div class="row">
                <div class="col-sm-2 col-xs-12 mt-15">
                    <a href="/St_ExtraExpense/Index" class="btn btn-rounded btn-block btn-anim" style="background-color:#E74C3C"><i class="fa fa-th"></i><span class="btn-text">@Resources.Resource.ExtraExpenses</span></a>
                </div>
            </div>
        </div>
    </div>
</div>
