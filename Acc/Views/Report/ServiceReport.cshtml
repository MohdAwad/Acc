            var PurchaseService = $("#PurchaseService:checked").val();
            var PurchaseMultiService = $("#PurchaseMultiService:checked").val();
            var ReturnPurchaseService = $("#ReturnPurchaseService:checked").val();
            var ReturnPurchaseMultiService = $("#ReturnPurchaseMultiService:checked").val();



